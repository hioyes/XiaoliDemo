<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <com.xiaoli.library.View.TitleBarLayout
        android:id="@+id/mTitleBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleBarTitle="@string/binner_label"
        app:titleBarBackgroundLeft="@drawable/shape_login_btn"
        />
    <com.recker.flybanner.FlyBanner
        android:id="@+id/mFlyBanner"
        android:layout_below="@id/mTitleBarLayout"
        android:layout_width="match_parent"
        android:layout_height="200dp"/>
    <com.recker.flybanner.FlyBanner
        android:id="@+id/mFlyBanner2"
        android:layout_below="@id/mFlyBanner"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        app:points_position="RIGHT"
        />


</RelativeLayout>
