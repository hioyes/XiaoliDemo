<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <com.xiaoli.library.View.TitleBarLayout
        android:id="@+id/mTitleBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleBarTitle="@string/demo_label"
        app:titleBarBackgroundLeft="@drawable/shape_login_btn"
        />
    <TextView
        android:id="@+id/mTvDemo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/mTitleBarLayout"
        android:text="@string/demo_label"/>


</RelativeLayout>
